class Matrice{
  
  public static boolean simmetrica?(int[][] mat) {
    
    for(int i=0; i<mat.length; i++){
      for(int j=0; j<mat.length; j++) {
        if(i==j){
          continue;
        } else {
          if(mat[i][j] != mat[j][i]){
            return false;
          }            
        }
      }
    }
    return true;
  }
  
}